{
  "name": "mean",
  "version": "1.0.0",
  "description": "This is a basic MEAN stack application that can be used as a boilerplate\"",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec 'npm run test:security && npm run test:unit && npm run lint && node' | gnomon --type=elapsed-total",
    "start:parallel:norefresh": "npm-run-all --parallel test:security test:unit lint serve | gnomon --type=elapsed-total",
    "start:parallel": "nodemon index.js --exec 'npm run parellel' | gnomon --type=elapsed-total",
    "parellel": "npm-run-all --parallel test:security test:unit lint",
    "serve": "nodemon index.js",
    "pretest:unit": "echo \"Unit testing...\"",
    "test:unit": "mocha '**/*.unit.test.js'",
    "pretest:security": "echo \"Security testing...\"",
    "test:security": "nsp check --reporter checkstyle",
    "prelint": "echo \"Linting...\"",
    "lint": "esw . --color",
    "start:fast": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rkristelijn/mean.git"
  },
  "author": "Remi Kristelijn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rkristelijn/mean/issues"
  },
  "homepage": "https://github.com/rkristelijn/mean#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "mongoose": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-watch": "^3.1.3",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0"
  }
}
